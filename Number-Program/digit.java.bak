import java.util.*;
class digit
{
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the Number");
		int n=s.nextInt();
		int sum=0;
		while (n>0)
		{
			sum+=(n%10);
			n=n/10;
		}
		System.out.println("Sum of Digits is "+sum);
	}
} 